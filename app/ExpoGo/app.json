{
  "expo": {
    "name": "Achei Barato",
    "slug": "achei-barato",
    "scheme": "ExpoGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.acheibarato"
    },
    "web": {
      "favicon": "src/assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typeRoutes": true
    },
    "plugins": [
      "expo-secure-store",
      "@react-native-google-signin/google-signin", 
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "6600ca8f-6208-454c-897a-3c6c919d99be"
      }
    }

  }
}
