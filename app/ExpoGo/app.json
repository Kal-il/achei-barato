{
  "expo": {
    "name": "ExpoGo",
    "slug": "ExpoGo",
    "scheme": "ExpoGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "center",
      "backgroundColor": "#F67235"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typeRoutes": true
    },
    "plugins": [
      "expo-secure-store",
      "expo-router"
    ]

  }
}
